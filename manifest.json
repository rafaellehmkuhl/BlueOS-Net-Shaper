{
  "name": "blueos-net-shaper",
  "description": "Network simulator for BlueOS: loss/latency/bandwidth",
  "version": "0.1.3",
  "type": "extension",
  "author": "Rafael Lehmkuhl",
  "company": "Blue Robotics",
  "url": "https://github.com/rafaellehmkuhl/blueos-net-shaper",
  "website": "https://github.com/rafaellehmkuhl/blueos-net-shaper",
  "readme": "https://raw.githubusercontent.com/rafaellehmkuhl/blueos-net-shaper/master/README.md",
  "icon": "mdi-network-strength-outline",
  "user_permissions": {},
  "docker": {
    "image": "rafaellehmkuhl/blueos-net-shaper:0.1.3",
    "network_mode": "host",
    "cap_add": ["NET_ADMIN", "NET_RAW"],
    "restart": "unless-stopped"
  },
  "ui": {
    "port": 8080,
    "path": "/"
  }
}

